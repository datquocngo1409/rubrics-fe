{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gts-fe": {
      "root": "e2e",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "@firebase/app",
              "firebase/app",
              "withinviewport",
              "chart.js",
              "lodash",
              "jquery",
              "'@firebase/auth",
              "firebase"
            ],
            "aot": true,
            "outputPath": "dist/gts-fe",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "src/assets/scss/style.scss",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "node_modules/angular-instantsearch/bundles/instantsearch.min.css",
              "node_modules/angular-instantsearch/bundles/instantsearch-theme-algolia.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/angular-calendar/css/angular-calendar.css",
              "node_modules/@swimlane/ngx-datatable/index.css",
              "node_modules/@swimlane/ngx-datatable/themes/material.css",
              "node_modules/@swimlane/ngx-datatable/assets/icons.css",
              "node_modules/nvd3/build/nv.d3.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/cryptocoins-icons/webfont/cryptocoins.css",
              "src/assets/fonts/data-table/icons.css",
              "node_modules/dragula/dist/dragula.css",
              "node_modules/@circlon/angular-tree-component/css/angular-tree-component.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/ngx-bar-rating/themes/br-default-theme.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              {
                "input": "node_modules/@progress/kendo-theme-material/dist/all.css"
              }
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/hammerjs/hammer.js",
              "node_modules/sortablejs/Sortable.js",
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/easy-pie-chart/dist/easypiechart.js",
              "node_modules/bootstrap/dist/js/bootstrap.js",
              "node_modules/jspdf-autotable/dist/jspdf.plugin.autotable.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [{
                "type": "anyComponentStyle",
                "maximumWarning": "6kb"
              }],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "fileReplacements": [{
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.prod.ts"
              }]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gts-fe:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gts-fe:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gts-fe:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/hammerjs/hammer.js",
              "node_modules/sortablejs/Sortable.js",
              "node_modules/chart.js/dist/Chart.bundle.min.js",
              "node_modules/easy-pie-chart/dist/easypiechart.js",
              "node_modules/bootstrap/dist/js/bootstrap.j"
            ],
            "styles": [
              "src/assets/scss/style.scss",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "node_modules/angular-instantsearch/bundles/instantsearch.min.css",
              "node_modules/angular-instantsearch/bundles/instantsearch-theme-algolia.min.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/angular-calendar/css/angular-calendar.css",
              "node_modules/@swimlane/ngx-datatable/release/index.css",
              "node_modules/@swimlane/ngx-datatable/release/themes/material.css",
              "node_modules/@swimlane/ngx-datatable/assets/icons.css",
              "node_modules/nvd3/build/nv.d3.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/cryptocoins-icons/webfont/cryptocoins.css",
              "src/assets/fonts/data-table/icons.css",
              "node_modules/dragula/dist/dragula.css",
              "node_modules/@circlon/angular-tree-component/css/angular-tree-component.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/ngx-bar-rating/themes/br-default-theme.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              {
                "input": "node_modules/@progress/kendo-theme-material/dist/all.css"
              }
            ],
            "assets": [
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "gts-fe-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "gts-fe:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "gts-fe",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "gts-fe",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "gts-fe"
    }
  },
  "cli": {
    "analytics": "720b99e8-9c05-4573-ac66-0fe468e383bc"
  }
}
